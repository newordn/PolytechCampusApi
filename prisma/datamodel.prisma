type Crew{
    id: ID! @id
    title: String!
    description:String!
    users: [User] @relation(name:"CrewUser")
}
type User{
    id: ID! @id
    name:String!
    matricule: String! @unique
    email: String! @unique
    filiere: String!
    option: String!
    password: String!
    crews: [Crew] @relation(name:"CrewUser")
}